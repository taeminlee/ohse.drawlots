<Window x:Class="ohse.drawlots.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ohse.drawlots"
        mc:Ignorable="d"
        Title="LoginWindow" Height="137.447" Width="350.213">
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <PasswordBox Name="PW" KeyDown="PW_OnKeyDown" Margin="10" PasswordChar="*"></PasswordBox>
        <Button Grid.Row="1" Click="LoginClick">Login</Button>
    </Grid>
</Window>
